
numOfRounds = int(input())
mishka = 0
chris = 0
for i in range (numOfRounds):
    x,y = list(map(int,input().split()))
    if x>y:
        mishka+=1
    elif x<y:
        chris+=1
    else:
        continue
if (mishka > chris):
    print("Mishka")
elif chris>mishka:
    print("Chris")
else:
    print("Friendship is magic!^^")
